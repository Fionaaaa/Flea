<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView32_my_emption_classify"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="25dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="25dp"
        android:text="[求购：电器]"
        android:textColor="@color/blue_m"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView33_my_emption_describe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView32_my_emption_classify"
        android:layout_marginLeft="5dp"
        android:layout_toEndOf="@+id/textView32_my_emption_classify"
        android:layout_toRightOf="@+id/textView32_my_emption_classify"
        android:ellipsize="end"
        android:maxLength="10"
        android:text="跪求一台电风扇"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView34_my_emption_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView33_my_emption_describe"
        android:layout_marginRight="5dp"
        android:text="下午1:07"
        android:textColor="@color/black_h"
        android:textSize="10dp" />

    <com.fiona.tiaozao.fragment.myself.edit.SureCircleView
        android:id="@+id/sureCirleView_emption"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/textView33_my_emption_describe"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:visibility="invisible"
        android:layout_marginRight="10dp" />
</RelativeLayout>